    "import wikipediaapi\n",
    "import nltk\n",
    "from nltk.tokenize import word_tokenize\n",
    "from nltk.stem import WordNetLemmatizer\n",
    "from collections import Counter\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from nltk.corpus import stopwords\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "798b9cf2-57a1-4171-a476-758b11382c07",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1000x600 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Wikipedia API nesnesini oluşturuyoruz\n",
    "wiki_wiki = wikipediaapi.Wikipedia(user_agent=\"MyApp/1.0 (myemail@example.com)\", language=\"en\")\n",
    "\n",
    "# Dosyayı açıp içeriğini sözlük olarak okumalıyız\n",
    "sanatci_listesi = {}\n",
    "\n",
    "with open(\"sanatcilar_biyografi.txt\", \"r\", encoding=\"utf-8\") as file:\n",
    "    sanatci_adı = None\n",
    "    sanatci_biyografi = \"\"\n",
    "    for line in file:\n",
    "        if line.startswith(\"---\"):  # Sanatçı isimleri böyle formatlanmış olabilir\n",
    "            if sanatci_adı:  # Önceki sanatçının biyografisini kaydet\n",
    "                sanatci_listesi[sanatci_adı] = sanatci_biyografi.strip()\n",
    "            sanatci_adı = line.strip(\"---\").strip()  # Sanatçının adını al\n",
    "            sanatci_biyografi = \"\"  # Yeni sanatçı için sıfırla\n",
    "        else:\n",
    "            sanatci_biyografi += line  # Biyografi satırlarını birleştir\n",
    "\n",
    "    if sanatci_adı:  # Son sanatçıyı da ekleyelim\n",
    "        sanatci_listesi[sanatci_adı] = sanatci_biyografi.strip()\n",
    "\n",
    "# Tüm sanatçıların metinlerini birleştiriyoruz\n",
    "combined_text = \" \".join(sanatci_listesi.values())\n",
    "\n",
    "# Kelime tokenizasyonu\n",
    "word_tokens = word_tokenize(combined_text)\n",
    "\n",
    "# Stopwordleri kaldırmak için stopword listesini çekelim\n",
    "stop_words = set(stopwords.words('english'))\n",
    "filtered_words = [word for word in word_tokens if word.isalpha() and word.lower() not in stop_words]\n",
    "\n",
    "# **Lemmatizer'ı başlatıyoruz**\n",
    "lemmatizer = WordNetLemmatizer()\n",
    "\n",
    "# Lemmatize edilmiş kelimeleri içeren liste\n",
    "lemmatized_words = [lemmatizer.lemmatize(word.lower()) for word in filtered_words]\n",
    "\n",
    "# Benzersiz lemmatize edilmiş kelimeler\n",
    "unique_lemmatized_words = set(lemmatized_words)\n",
    "\n",
    "# **Lemmatizasyon sonrası log-log grafiği çiziyoruz**\n",
    "word_freq = Counter(lemmatized_words)\n",
    "\n",
    "# Kelimelerin ve frekanslarının sıralanması\n",
    "words, freqs = zip(*word_freq.items())\n",
    "\n",
    "# Frekansları logaritmik olarak çiz (Eksenler yer değiştirilmiş)\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.loglog(range(len(freqs)), freqs, marker='.', linestyle='None')\n",
    "plt.title('Zipf Yasası - Log-Log Grafiği (Eksenler Ters)')\n",
    "plt.xlabel('Kelime sıralaması (log)')\n",
    "plt.ylabel('Kelime frekansı (log)')\n",
    "plt.grid(True)\n",
    "plt.show()\n",
    "\n",
    "\n"
   ]
  },
